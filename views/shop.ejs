<!-- ========== MAIN CONTENT ========== -->
<main id="content" role="main">

    <div class="container">
        <br><br>
        <div class="row mb-8">
            
            <div class="border-bottom border-color-1">
                <h3 class="section-title section-title__full d-inline-block mb-0 pb-2 font-size-22">Product List</h3>
            </div>
                <!-- Shop Body -->
                <!-- Tab Content -->
                <div class="tab-pane fade pt-2 show active" id="pills-three-example1" role="tabpanel"
                    aria-labelledby="pills-three-example1-tab" data-target-group="groups">
                    <ul class="d-block list-unstyled products-group prodcut-list-view">
                        <% results.forEach(function(result) {%>
                        <li class="product-item remove-divider">
                            <div class="product-item__outer w-100">
                                <div class="product-item__inner remove-prodcut-hover py-4 row">
                                    <div class="product-item__header col-6 col-md-4">
                                        <div class="mb-2">
                                            <a href="/dproduct/?id=<%=result.model_no%>"
                                                class="d-block text-center"><img class="img-fluid"
                                                    src="<%= result.img1 %>" alt="Image Description"></a>
                                        </div>
                                    </div>
                                    <div class="product-item__footer col-md-3 d-md-block"></div>
                                    <div class="product-item__body col-6 col-md-5">
                                        <div class="pr-lg-10">
                                            <div class="mb-2"><a href="/dproduct/?id=<%=result.model_no%>"
                                                    class="font-size-12 text-gray-5"><%= result.brand %></a></div>
                                            <h5 class="mb-2 product-item__title"><a
                                                    href="/dproduct/?id=<%=result.model_no%>"
                                                    class="text-blue font-weight-bold"><%= result.s_des %></a></h5>

                                            <div class="mb-3 d-none d-md-block">
                                                <a class="d-inline-flex align-items-center small font-size-14" href="#">
                                                    <div class="text-warning mr-2">
                                                        <% for (var i=0; i<result.rating; i++){%>
                                                            <small class="fas fa-star"></small>
                                                        <%}%>
                                                        <% for (var i=0; i<5-result.rating; i++){%>
                                                            <small class="far fa-star text-muted"></small>
                                                        <%}%>
                                                    </div>
                                                </a>
                                            </div>
                                            <ul class="font-size-12 p-0 text-gray-110 mb-4 d-none d-md-block">
                                                <li class="line-clamp-1 mb-1 list-bullet"><%= result.a_1 %></li>
                                                <li class="line-clamp-1 mb-1 list-bullet"><%= result.a_2 %></li>
                                                <li class="line-clamp-1 mb-1 list-bullet"><%= result.a_3 %></li>
                                            </ul>
                                        </div>
                                        <br><br><br><br><br><br><br>
                                        <div class="mb-3">
                                            <div class="prodcut-price mb-2">
                                                <div class="text-gray-100">₹<%= result.mrp %></div>
                                            </div>
                                            <div class="prodcut-add-cart">
                                                <a href="/addtowishlist/?email=<%=email%>&model_no=<%=result.model_no%>"
                                                    class="btn btn-sm btn-block btn-primary-dark btn-wide transition-3d-hover"><i
                                                        class="fa fa-bookmark mr-2 font-size-18"></i> Save Product</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <% }); %>
                    </ul>
                </div>
                <!-- End Tab Content -->
                <!-- End Shop Body -->
            </div>
        </div>
    </div>
</main>
<!-- ========== END MAIN CONTENT ========== -->

